# Payment Service Environment Variables

# Database
DATABASE_URL="postgresql://damocles:password@localhost:5432/damocles"

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Vipps Configuration (Norwegian mobile payments)
VIPPS_CLIENT_ID=your_vipps_client_id
VIPPS_CLIENT_SECRET=your_vipps_client_secret
VIPPS_SUBSCRIPTION_KEY=your_vipps_subscription_key
VIPPS_MERCHANT_SERIAL_NUMBER=your_merchant_serial_number

# Service Configuration
PORT=8009
HOST=0.0.0.0
NODE_ENV=development

# Application URLs
WEB_URL=http://localhost:3001

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# Winston Logging
LOG_LEVEL=info